#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>

void selectionSort(int v[], int n){
    for(int i=0; i<n-1; i++){
        int min_idx = i;

        for(int j=i+1; j<n; j++){
            if(v[j] < v[min_idx]){
                min_idx = j;
            }
        }

        int tmp = v[i];
        v[i] = v[min_idx];
        v[min_idx] = tmp;
    }
}

int main(){
    int n = 5;
    int v[5] = {0}; 

    clock_t start = clock();

    selectionSort(v, n); 

    clock_t end = clock();

    double tmp = (double)(end - start) / CLOCKS_PER_SEC;

    printf("Execution time: %f seconds\n", tmp);

    return 0;

}
